    <body id="SignUp">
        <link rel="stylesheet" type="text/css" href="../../assests/css/su.css">
        <div class="container">
            <h1 class="form-heading">POS AND INVENTORY</h1>
            <div class="login-form">
                <div class="main-div">
                    <div class="panel">
                        <h2>Gearup Nepal</h2>
                        <p>Please enter your Information</p>
                    </div>
                    <form method="post">

                        <div class="form-group">


                            <input type="name" class="form-control" id="inputEmail" name="userName" placeholder="UserName">

                        </div>

                        <div class="form-group">


                            <input type="fname" class="form-control" id="inputEmail" name="fullName" placeholder="FullName">

                        </div>

                        <div class="form-group">


                            <input type="email" class="form-control" id="inputEmail"  name="email" placeholder="Email Address">

                        </div>

                        <div class="form-group">

                            <input type="password" class="form-control" id="inputPassword"  name="password" placeholder="Password">

                        </div>

                        <div class="form-group">


                            <input type="mob" class="form-control" id="inputEmail"  name="mobileNo" placeholder="Mobile Number">

                        </div>

                        <button type="submit" class="btn btn-primary">SignUp</button>

                    </form>
                </div>
            </div></div></div>


